declare TITLE "FizzBuzz Example";
declare AUTHOR "Gren Drake";
declare VERSION 1;
declare GAMEID "";

function main() {
    [ i ]

    ("See https://imranontech.com/2007/01/24/using-fizzbuzz-to-find-developers-who-grok-coding/ or another page about FizzBuzz in programming for an explanation of this program.\n")
    (set i 1)
    (while (lte i 100)
        (proc
            (if (eq (mod i 15) 0)
                (print "FizzBuzz")
                (if (eq (mod i 3) 0)
                    (print "Fizz")
                    (if (eq (mod i 5) 0)
                        (print "Buzz")
                        (print i))))
            (print "[br]")
            (inc i)))

}
