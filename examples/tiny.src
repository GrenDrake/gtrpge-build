object magic_numbers;

asm_function main(fromRestore) {
    "Tiny Demo Game" InfobarTitle set_info

    10000 40000 random $number1 magic_numbers set_prop
    10000 40000 random $number2 magic_numbers set_prop
    10000 40000 random $number3 magic_numbers set_prop

    "Todays magic numbers are: " say
    $number1 magic_numbers get_prop say
    ", " say
    $number2 magic_numbers get_prop say
    ", and " say
    $number3 magic_numbers get_prop say
    ".\nGoodbye!" say
}
